{
  "manifest_version": 2,

  "name": "Multi-Search",
  "description": "Use keyword 'multi' followed by your custom search keyword to search a list of sites for your term.",
  "version": "1.0",
  "minimum_chrome_version": "9",
  "omnibox": { "keyword" : "multi" },
  "options_page": "options.html",
  
  "permissions": ["storage"],
  
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  
  "icons": {
	"16": "MS 16.png",
	"128": "MS 16.png",
    "32": "MS 16.png",
    "48": "MS 16.png"
  },
  
  "browser_action": {
	"default_icon": {
		"19": "MS 19.png",
		"38": "MS 38.png"
	},
	"default_title": "Multi-Search (options)"
  }
}
